<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.og.Controllers.SellerScreenController">
   <children>
      <Label fx:id="fullNameLabel" layoutX="32.0" layoutY="14.0" text="Label" />
      <TableView fx:id="booksInShopTable" layoutX="150.0" layoutY="92.0" prefHeight="200.0" prefWidth="299.0">
        <columns>
          <TableColumn fx:id="titleColumn" prefWidth="75.0" text="Title" />
          <TableColumn fx:id="authorColumn" prefWidth="75.0" text="Author" />
            <TableColumn fx:id="genreColumn" prefWidth="75.0" text="Genre" />
            <TableColumn fx:id="amountColumn" prefWidth="75.0" text="Amount" />
        </columns>
      </TableView>
      <Button layoutX="34.0" layoutY="338.0" mnemonicParsing="false" onAction="#onButtonLogOutClick" text="Log out" />
      <Button layoutX="441.0" layoutY="303.0" mnemonicParsing="false" onAction="#onButtonSellClick" text="Sell" />
      <Button layoutX="421.0" layoutY="338.0" mnemonicParsing="false" text="From stock" />
      <Button onAction="#onButtonNeedItClick" layoutX="470.0" layoutY="200.0" mnemonicParsing="false" text="Need it" />
   </children>
</AnchorPane>
