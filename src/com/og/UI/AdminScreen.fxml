      </TableView>
      <Label fx:id="fullNameLabel" layoutX="74.0" layoutY="47.0" text="Label" />
      <Button layoutX="285.0" layoutY="466.0" mnemonicParsing="false" onAction="#addNewProductButtonClick" text="Add new product" />
      <Button layoutX="591.0" layoutY="649.0" mnemonicParsing="false" onAction="#logOutButton" prefHeight="26.0" prefWidth="94.0" text="Log out" />
      <Button layoutX="322.0" layoutY="649.0" mnemonicParsing="false" onAction="#onButtonBackClick" text="Back" />
